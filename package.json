{
  "name": "@warren-bank/browser-castv2",
  "version": "1.0.0",
  "description": "Browser build for 'node-castv2': An implementation of the Chromecast CASTV2 protocol",
  "license": "GPL-2.0",
  "main": "dist/es2020/castv2.js",
  "files": [
    "dist/es2020/*"
  ],
  "scripts": {
    "build:es2020": "webpack --config webpack.es2020.config.js",
    "build:es5":    "webpack --config webpack.es5.config.js",
    "build":        "npm run build:es2020 && npm run build:es5"
  },
  "dependencies": {
    "@warren-bank/net-browserify": "1.0.0",
    "@warren-bank/url": "3.2.1",
    "buffer": "6.0.3",
    "castv2": "0.1.10",
    "events": "3.3.0",
    "stream-browserify": "3.0.0",
    "timers-browserify": "2.0.12",
    "tls-browserify": "0.2.2",
    "util": "0.12.5"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.22.20",
    "babel-loader": "^9.1.3",
    "webpack-cli": "^5.1.4",
    "webpack": "^5.88.2"
  },
  "author": {
    "name": "Warren R. Bank",
    "email": "warren.r.bank@gmail.com",
    "url": "https://github.com/warren-bank"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/warren-bank/browser-castv2"
  },
  "keywords": [
    "browser",
    "chromecast",
    "castv2"
  ]
}
